{
  "name": "klubs",
  "version": "0.0.2",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env BUNDLE_ANALYZE=both next build",
    "find:unused": "next-unused"
  },
  "sideEffects": false,
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@stripe/stripe-js": "^1.37.0",
    "@supabase/supabase-auth-helpers": "^1.2.3",
    "@supabase/supabase-js": "^1.35.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "clsx": "^1.2.0",
    "cross-env": "^7.0.3",
    "focus-visible": "^5.2.0",
    "html-react-parser": "^3.0.4",
    "js-cookie": "^3.0.1",
    "next": "^12.3.1",
    "next-themes": "^0.2.1",
    "postcss-focus-visible": "^7.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-merge-refs": "^2.0.1",
    "react-player": "^2.11.0",
    "stripe": "^10.11.0",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.21",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "lint-staged": "^13.0.1",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "**/*.{md,mdx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "next-unused": {
    "alias": {
      "@/lib/*": [
        "lib/*"
      ],
      "@/components/*": [
        "components/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
